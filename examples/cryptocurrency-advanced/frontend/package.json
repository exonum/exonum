    "http-proxy-middleware": "^0.19.1",
    "jquery": "^3.4.1",
    "noty": "^3.2.0-beta",
    "numeral": "^2.0.6",
    "popper.js": "^1.16.1",
    "protobufjs": "^6.8.8",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2",
    "yargs-parser": "^13.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "^7.0.0-bridge.0",
